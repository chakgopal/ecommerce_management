<h1><center>Sellers</center></h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th>id</th> 
      <th>email</th> 
      <th>name</th>
      <th>role</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sellers.each do |seller| %>
      <tr>
        <td><%= seller.id %></td>
        <td><%= seller.email %></td>
        <td><%= seller.name %></td>
        <td><%= seller.role %></td>
        <td><%= link_to 'Show', seller %></td>
        <td><%= link_to 'Edit', edit_seller_path(seller) %></td>
        <td><%= link_to 'Destroy', seller, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

